# Next.js base URL
NEXT_PUBLIC_APP_URL=https://app.santrionline.com
NEXTAUTH_URL=https://app.santrionline.com
NEXTAUTH_SECRET=changeme-nextauth-secret
AUTH_SECRET=changeme-auth-secret

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Credential auth salt
BCRYPT_SALT_ROUNDS=10

# Database
DATABASE_URL=file:./prisma/dev.db

# Cloudflare bindings (in production provided by CF Pages/Workers env)
CF_PAGES_BRANCH=main
MOONWA_API_URL=https://api.moonwa.com/v1/messages
MOONWA_API_KEY=your-moonwa-token

# R2
R2_BUCKET_NAME=santri-online-files
R2_PUBLIC_ENDPOINT=https://<accountid>.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-access-key
R2_SECRET_ACCESS_KEY=your-secret
